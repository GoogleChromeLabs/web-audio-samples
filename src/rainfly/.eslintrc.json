{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:svelte/recommended",
    "google"
  ],
  "ignorePatterns": [
    "build/",
    ".svelte-kit/",
    "dist/",
    "static/"
  ],
  "globals": {
    "window": "readonly",
    "document": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {}
}