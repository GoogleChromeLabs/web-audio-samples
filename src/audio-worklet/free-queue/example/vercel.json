{
    "headers": [{
        "source": "/(.*)",
        "headers": [
            {
                "key": "Cross-Origin-Embedder-Policy",
                "value": "require-corp"
            }, 
            {
                "key": "Cross-Origin-Opener-Policy",
                "value": "same-origin"
            },
            {
                "key" : "Cache-Control",
                "value" : "max-age=0"
            }
        ]
    }]
}